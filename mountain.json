{
  "speed": [
    {
      "if": "road_class == PATH",
      "multiply_by": 1.3
    },
    {
      "if": "road_class == TRACK",
      "multiply_by": 1.4
    },
    {
      "if": "surface == DIRT",
      "multiply_by": 1.2
    },
    {
      "if": "surface == GRAVEL",
      "multiply_by": 1.1
    },
    {
      "if": "mtb_scale == 0",
      "multiply_by": 1.2
    },
    {
      "if": "mtb_scale == 1",
      "multiply_by": 1.0
    },
    {
      "if": "mtb_scale >= 2",
      "multiply_by": 0.8
    }
  ],
  "priority": [
    {
      "if": "road_class == PATH",
      "multiply_by": 2.0
    },
    {
      "if": "road_class == TRACK",
      "multiply_by": 1.8
    },
    {
      "if": "road_class == PRIMARY",
      "multiply_by": 0.3
    },
    {
      "if": "road_class == TRUNK",
      "multiply_by": 0.1
    }
  ]
} 